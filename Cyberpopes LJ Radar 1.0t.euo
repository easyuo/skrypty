sub replace
namespace push
namespace local rep
for !i 1 1000
   {
   str len %2
   set !par2len #strres
   set !s %1
   str len %1
   set !len #strres
      str pos %1 %2
   if #strres = 0
      {
      set #strres %1
      namespace clear
      namespace pop
      return
      }
   else
      {
      set !p #strres
      str del %1 !p !len
      set %1 #strres
      set !p !p + !par2len - 1
      str del !s 1 !p
      set %1 %1 , %3 , #strres
      }
   }

